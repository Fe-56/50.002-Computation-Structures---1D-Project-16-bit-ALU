module ROM2 (
    input selector[5],    // 5 bits selector
    output out[19]         // 19 bits output (S+Z+V+N)  
  ) {
  
  const INPUTS = {b0000000000101010000, b1111000010010110000, b0000010100011010000, b1011011110100000000,b0010110010001010000,b1111011011101000000,
  b0101101110111010000, b0000000000010001000,b0000000000000000000,b0000000000000000000, b0000000000000001100,b0000000000000001001,b0000000000000000000,
    b0000000000000001001,b0000000000000000000,b0000000000000001100,b0000000000000000100,b0000000000000001010,b1000000000000000011,b0000000000000010000, 
    b0000000000000000100,b0110111001010010010,b1101010011001011011,b0000000000010000000};

  always {
    out = INPUTS[selector];
  }
}

