module boolean (
    input alufn[6],    
    input a[16],
    input b[16],
    
    output out[16]
  ) {

  always {
  
    case(alufn[3:0]){
     // AND
     b1000 : out = a & b;
      //OR
     b1110 : out = a | b;
      //XOR
     b0110 : out = a ^ b;
      //A
     b1010 : out = a;
     default:
      out = 0;
    }
  }
}
